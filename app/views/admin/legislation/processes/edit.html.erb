<% provide :title do %>
  <%= t("admin.header.title") %> - <%= t("admin.menu.legislation") %> - <%= @process.title %>
<% end %>

<div class="legislation-process-edit">
  <%= back_link_to polymorphic_path([@namespace, :legislation_processes]), t("admin.legislation.processes.edit.back") %>

  <h2><%= @process.title %></h2>

  <%= render "admin/legislation/processes/subnav", process: @process, active: "info" %>

  <%= render "admin/legislation/processes/form" %>
</div>
